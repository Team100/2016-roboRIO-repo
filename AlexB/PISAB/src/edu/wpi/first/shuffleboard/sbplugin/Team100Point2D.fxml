<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import edu.wpi.first.shuffleboard.sbplugin.*?>
<GridPane xmlns="http://javafx.com/javafx/8.0.121"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="edu.wpi.first.shuffleboard.sbplugin.Team100Point2DWidget"
          fx:id="root"
          prefHeight="128" prefWidth="256" minHeight="96" minWidth="192"> 
    <padding>
        <Insets topRightBottomLeft="8"/>
    </padding>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" percentHeight="50"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" percentHeight="50"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="64.0" percentWidth="30.0" prefWidth="100.0"/>
        <ColumnConstraints minWidth="64.0" percentWidth="70.0" prefWidth="100.0"/>
    </columnConstraints>
    <Label text="X" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Label text="Y" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
    <TextField editable="false" fx:id="XField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <TextField editable="false" fx:id="YField" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
</GridPane>